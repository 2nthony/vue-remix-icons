{
  "name": "vue-remix-icons",
  "description": "Remix Icon is a set of open source neutral style system symbols elaborately crafted for designers and developers. Powered by remixicon.",
  "version": "1.0.0",
  "funding": "https://github.com/sponsors/2nthony",
  "main": "./index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "repository": "https://github.com/2nthony/vue-remix-icons",
  "author": "2nthony",
  "license": "Apache-2.0",
  "keywords": [
    "vue",
    "vue 2",
    "vue 3",
    "remixicon",
    "remix",
    "icon",
    "icon library",
    "icon pack"
  ],
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "icons",
    "index.js",
    "index.d.ts"
  ],
  "exports": {
    ".": {
      "require": "./index.js",
      "import": "./index.js",
      "types": "./index.d.ts"
    },
    "./icons/*": {
      "require": "./icons/*",
      "import": "./icons/*",
      "types": "./index.d.ts"
    }
  },
  "sideEffects": false,
  "scripts": {
    "gen": "esno scripts/gen.ts",
    "build": "npm run gen",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@2nthony/prettier-config": "1.0.1",
    "@types/node": "^18.7.6",
    "esno": "^0.16.3",
    "pascal-case": "^3.1.2",
    "prettier": "2.7.1",
    "remixicon": "^2.5.0",
    "typescript": "4.7.4",
    "vue": "^3.2.37"
  }
}
